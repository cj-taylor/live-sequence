{
  "name": "Live Sequence",
  "version": "1.1",
  "description": "Adds a new panel in the devtools of chrome browser that displays an realtime sequence diagram of the execution flow.",
  "devtools_page": "devtools.html",
  "manifest_version": 2,
  "default_locale": "en",
  "offline_enabled": true,
  "options_page": "options.html",
  "icons": {"48": "live-sequence.png"},
  "background": {
	"scripts": ["background.js"],
	"persistent": false
   },
  "permissions": ["experimental", "tabs", "http://*/*", "https://*/*"]
}
